reset

set term gif
set output "/home/pi/Documents/Temperature/temperaturehistory.gif"

set yrange [-30:30]
set ytics (30, 20, 10, 0, -10, -20, -30)
set ylabel "Celsius" offset 1
set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"

set xrange [time(0) - 7*24*60*60:]
set format x "%Y\n%d-%m"

set lmargin 9
set rmargin 2
set grid

plot "/home/pi/Documents/Temperature/temperature.log" using 1:6 notitle with lines
